{
    "manifest_version": 2,

    "name": "测试插件",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",

    "permissions": [
        "activeTab",
        "notifications",
        "https://image.baidu.com/"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Click here!"
    }
}
